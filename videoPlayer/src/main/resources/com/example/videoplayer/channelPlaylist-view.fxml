<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.videoplayer.Panel.ChannelPlaylistPanel">
    <children>
        <ScrollPane fitToWidth="true" layoutX="88.0" prefHeight="500.0" prefWidth="809.0">
            <content>
                <VBox fx:id="playlistContainer" prefHeight="507.0" prefWidth="618.0" />
            </content>
        </ScrollPane>
        <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="88.0" spacing="20.0" style="-fx-background-color: #1e1e1e;">
            <children>
                <ImageView fitHeight="90.0" fitWidth="69.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../Image/1000043271-removebg-preview.png" />
                    </image>
                </ImageView>
                <Button fx:id="homeBtn1" mnemonicParsing="false" style="-fx-background-color: transparent;" text="🏠" textFill="WHITE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
                <Button fx:id="libraryBtn1" mnemonicParsing="false" style="-fx-background-color: transparent;" text="📚" textFill="WHITE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
                <Button fx:id="subscriptionBtn1" mnemonicParsing="false" style="-fx-background-color: transparent;" text="📁" textFill="WHITE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
                <Button fx:id="channelBtn1" mnemonicParsing="false" style="-fx-background-color: transparent;" text="👤" textFill="WHITE">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
            </children>
            <padding>
                <Insets bottom="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
