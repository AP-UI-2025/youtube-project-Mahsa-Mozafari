<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: #1e1e1e;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.videoplayer.ViewController.HomeController">
   <children>

      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="135.0" spacing="30.0">
         <children>
            <Button fx:id="homeBtn" mnemonicParsing="false" text="🏠">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Home" />
            <Button fx:id="libraryBtn" mnemonicParsing="false" text="📚">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Text fill="#fcf9f9" strokeType="OUTSIDE" strokeWidth="0.0" text="Library" />
            <Button fx:id="subscriptionBtn" mnemonicParsing="false" text="👩‍💻">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Text fill="#f7f4f4" strokeType="OUTSIDE" strokeWidth="0.0" text="Subscriptions" />
            <Button fx:id="channelBtn" mnemonicParsing="false" text="💻">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Text fill="#fcf8f8" strokeType="OUTSIDE" strokeWidth="0.0" text="Channel" />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="135.0" layoutY="23.0" prefHeight="518.0" prefWidth="646.0" spacing="10.0">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="36.0" prefWidth="646.0">
               <children>
                  <TextField fx:id="searchField" prefHeight="26.0" prefWidth="616.0" promptText="search" />
                  <Button fx:id="searchBtn" mnemonicParsing="false" onAction="#onSearch" text="🔎​" />
               </children>
            </HBox>
            <ScrollPane fitToWidth="true" prefHeight="398.0" prefWidth="595.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="460.0" prefWidth="558.0">
                     <children>
                        <VBox fx:id="mainContainer" prefHeight="461.0" prefWidth="632.0" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
