<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #1e1e1e;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.videoplayer.Controller.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="78.0" layoutY="19.0" prefHeight="360.0" prefWidth="462.0" spacing="20.0" style="-fx-border-color: gray; -fx-border-width: 3;">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Label text="Login" textFill="#f7f5f5" />
            <TextField fx:id="usernameText" promptText="username" />
            <PasswordField fx:id="passwordText" promptText="password" />
            <Button mnemonicParsing="false" onAction="#loginAct" text="Login" />
            <Button mnemonicParsing="false" onAction="#backToStart" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
