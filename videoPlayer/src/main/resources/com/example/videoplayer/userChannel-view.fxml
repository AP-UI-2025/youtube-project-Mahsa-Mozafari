          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="394.0" prefWidth="647.0">
               <children>
                  <VBox fx:id="playlistContainer" prefHeight="394.0" prefWidth="629.0" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <Button layoutX="173.0" layoutY="522.0" mnemonicParsing="false" text="Create Playlist" />
      <Button layoutX="375.0" layoutY="522.0" mnemonicParsing="false" text="Publish Content" />
