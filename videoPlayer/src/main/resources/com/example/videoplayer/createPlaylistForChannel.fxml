<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.videoplayer.Panel.CreatePlaylistForChannelPanel">
    <children>
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300.0" prefWidth="450.0" spacing="60.0" style="-fx-border-radius: 5; -fx-background-color: gray;">
            <children>
                <Label text="New Playlist">
                    <font>
                        <Font name="Segoe UI Bold" size="21.0" />
                    </font>
                </Label>
                <TextField fx:id="playlistNameField" prefHeight="47.0" prefWidth="450.0" promptText="Title:" />
                <HBox alignment="CENTER" prefHeight="58.0" prefWidth="430.0" spacing="60.0">
                    <children>
                        <Button fx:id="handleCancel" mnemonicParsing="false" style="-fx-background-color: #ff0000;" text="Cancel" textFill="WHITE" />
                        <Button mnemonicParsing="false" onAction="#handleCreatePlaylistForChannel" style="-fx-background-color: #1e1e1e;" text="Create" textFill="WHITE" />
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
